{
  "name": "shrinkx",
  "version": "0.0.3",
  "description": "",
  "main": "./out/main/index.js",
  "author": "ahm0xc",
  "homepage": "https://github.com/ahm0xc/shrinkx",
  "scripts": {
    "format": "prettier --write .",
    "lint": "eslint --cache .",
    "typecheck:node": "tsc --noEmit -p tsconfig.node.json --composite false",
    "typecheck:web": "tsc --noEmit -p tsconfig.web.json --composite false",
    "typecheck": "npm run typecheck:node && npm run typecheck:web",
    "start": "electron-vite preview",
    "dev": "electron-vite dev",
    "build": "npm run typecheck && electron-vite build",
    "postinstall": "electron-builder install-app-deps",
    "build:unpack": "npm run build && electron-builder --dir",
    "build:win": "npm run build && electron-builder --win",
    "build:mac": "npm run clean && electron-vite build && electron-builder --mac",
    "build:linux": "npm run clean && electron-vite build && electron-builder --linux",
    "clean": "npx rimraf out dist"
  },
  "dependencies": {
    "@electron-toolkit/preload": "^3.0.1",
    "@electron-toolkit/utils": "^4.0.0",
    "electron-download-manager": "^2.1.2",
    "nanoid": "^5.1.3",
    "sharp": "^0.33.5",
    "unzipper": "^0.12.3"
  },
  "devDependencies": {
    "@electron-toolkit/eslint-config-prettier": "^3.0.0",
    "@electron-toolkit/eslint-config-ts": "^3.0.0",
    "@electron-toolkit/tsconfig": "^1.0.1",
    "@radix-ui/react-dialog": "^1.1.6",
    "@radix-ui/react-dropdown-menu": "^2.1.6",
    "@radix-ui/react-slider": "^1.2.3",
    "@radix-ui/react-slot": "^1.1.2",
    "@radix-ui/react-switch": "^1.1.3",
    "@types/node": "^22.13.4",
    "@types/react": "^18.3.18",
    "@types/react-dom": "^18.3.5",
    "@types/unzipper": "^0.10.11",
    "@vitejs/plugin-react": "^4.3.4",
    "autoprefixer": "^10.4.21",
    "class-variance-authority": "^0.7.1",
    "clsx": "^2.1.1",
    "electron": "^35.0.1",
    "electron-builder": "^25.1.8",
    "electron-vite": "^3.0.0",
    "eslint": "^9.20.1",
    "eslint-plugin-react": "^7.37.4",
    "eslint-plugin-react-hooks": "^5.1.0",
    "eslint-plugin-react-refresh": "^0.4.19",
    "input-otp": "^1.4.2",
    "lucide-react": "^0.479.0",
    "postcss": "^8.5.3",
    "prettier": "^3.5.1",
    "react": "^18.3.1",
    "react-compare-slider": "^3.1.0",
    "react-dom": "^18.3.1",
    "rimraf": "^6.0.1",
    "tailwind-merge": "^3.0.2",
    "tailwindcss": "^3.4.17",
    "tailwindcss-animate": "^1.0.7",
    "typescript": "^5.7.3",
    "use-local-storage": "^3.0.0",
    "vite": "^6.1.0"
  },
  "pnpm": {
    "ignoredBuiltDependencies": [
      "electron",
      "esbuild"
    ],
    "onlyBuiltDependencies": [
      "electron",
      "sharp"
    ]
  }
}
